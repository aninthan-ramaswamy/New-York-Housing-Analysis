a[which(diff_check == min(diff_check))]
factors_of_n <- c()
factors_n <- function(n)
{
for(i in 1:(n-1))
{
if((n %% i) == 0)
{
append(factors_of_n, i)
}
}
return(factors_of_n)
}
factors_of_n <- factors_n(10)
factors_of_n
